# outcome: sat

# Something is fishy with CVC4's handling of infinite quantifiers.
# Cf. JB's email from 21 January 2016.

data nat :=
  Zero
| Suc nat.

spec one : nat :=
  one = Suc Zero.

rec add2 : nat -> nat :=
  forall (n : nat). add2 n = Suc (Suc n).

pred [wf] p : nat -> prop :=
  forall (n : nat). p n => p (add2 n).

val n : nat.

goal p Zero.
