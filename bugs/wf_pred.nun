# expected: unknown (unsat)
# outcome: sat

# Something is fishy with CVC4's handling of infinite quantifiers.
# Cf. JB's email from 21 January 2016.

data nat :=
  Zero
| Suc nat.

spec one : nat :=
  one = Suc Zero.

rec plus : nat -> nat -> nat :=
  forall (n : nat). plus Zero n = n;
  forall (m : nat) (n : nat). plus (Suc m) n = Suc (plus m n).

pred [wf] p : nat -> prop :=
  forall (n : nat). p n => p (plus n (Suc (Suc Zero))).

val n : nat.

goal p Zero.
