# outcome: "Error in CVC4: Subexpressions must have a common base type:"
# expected: sat

# "The" and "x" obviously have the same type. Extensionality is needed to
# compare the two values here.

val a : type.

val x : (a -> prop) -> a.

spec The : (a -> prop) -> a :=
  forall (P : a -> prop) (x1 : a). (exists (xa : a). P xa && (forall (y : a). P y => (y = xa))) => (P x1 = (x1 = The P)).

goal ~ (x = The).
